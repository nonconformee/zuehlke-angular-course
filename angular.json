                "output": ""
              },
              {
                "glob": "**/*",
                "input": "projects/shared/src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "projects/shared/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/01-dependency-injection/src/environments/environment.ts",
                  "with": "projects/01-dependency-injection/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "01-dependency-injection:build:production"
            },
            "development": {
              "browserTarget": "01-dependency-injection:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "01-dependency-injection:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/01-dependency-injection/src/test.ts",
            "polyfills": "zone.js",
            "tsConfig": "projects/01-dependency-injection/tsconfig.spec.json",
            "karmaConfig": "projects/shared/karma.conf.js",
            "assets": [
              {
                "glob": "**/favicon.ico",
                "input": "projects/shared/src",
                "output": ""
              },
              {
                "glob": "**/*",
                "input": "projects/shared/src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "projects/shared/src/styles.css"
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "zuehlke-angular-course:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "zuehlke-angular-course:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/01-dependency-injection/src/**/*.ts",
              "projects/01-dependency-injection/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  },
  "schematics": {
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  }
}
